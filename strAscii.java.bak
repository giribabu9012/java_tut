class strAscii 
{
	public static void main(String[] args) 
	{
		String s="GiRi";
		char[]a=s.toCharArray();
		for (int i=0;i<a.length ;i++ )
		{
			if(a[i]>='A' && a[i]<='Z')
				a[i]=a[i]+32;
			else if(a[i]>='a' && a[i]<='z')
				a[i]=a[i]-32;
		}
		String b=new String(a);
		System.out.println(b);
	}
}
