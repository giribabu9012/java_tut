class neon 
{
	public static void main(String[] args) 
	{
		int a=9;
		int square=a*a;
		int sum=0;
		while(a>0)
		{
			int last=square % 10;
			sum=sum+last;
			square=square/10;
		}
		if(a==sum)
		{
		System.out.print("It is a neon number");
		}
		else
		{
		System.out.print("It is not a neon number");
		}	
	}
}
